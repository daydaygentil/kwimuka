{
  "compilerOptions": {
    "target": "es2020",
    "module": "esnext",
    "lib": ["es2020", "dom"],
    "types": ["@supabase/supabase-js"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "isolatedModules": true,
    "noImplicitAny": true,
    "skipLibCheck": true
  },
  "include": ["./**/*.ts", "../../../src/types/**/*.ts"],
  "exclude": ["node_modules"]
}
