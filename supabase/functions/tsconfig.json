{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "lib": ["ESNext", "dom"],
    "types": ["@supabase/supabase-js"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "strict": true,
    "noImplicitAny": true,
    "allowJs": true,
    "checkJs": false,
    "paths": {
      "https://deno.land/x/supabase_js@*": ["@supabase/supabase-js"]
    }
  },
  "include": ["**/*.ts"],
  "exclude": ["node_modules"]
}
